{
  "server": {
    "host": "0.0.0.0",
    "port": 443
  },
  "security": {
    "hsts_max_age": 31536000,
    "content_security_policy": "default-src 'self'; script-src 'self' 'unsafe-inline' https://cdn.jsdelivr.net; style-src 'self' 'unsafe-inline' https://cdn.jsdelivr.net; img-src 'self' data: https://cdn.jsdelivr.net https://fastapi.tiangolo.com"
  },
  "rate_limit": {
    "enabled": true,
    "rate_limit": "100/minute",
    "storage_uri": ""
  },
  "cors": {
    "enabled": false,
    "allow_origins": ["*"],
    "allow_credentials": true,
    "allow_methods": ["*"],
    "allow_headers": ["*"],
    "expose_headers": [],
    "max_age": 600
  },
  "certificate": {
    "directory": "certs",
    "ssl_keyfile": "key.pem",
    "ssl_certfile": "cert.pem",
    "days_valid": 365
  },
  "json_response": {
    "ensure_ascii": false,
    "allow_nan": false,
    "indent": null,
    "media_type": "application/json; charset=utf-8"
  }
}
